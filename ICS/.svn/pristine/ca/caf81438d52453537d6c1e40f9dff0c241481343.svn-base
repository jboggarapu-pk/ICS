    ''' </list>
    ''' </remarks>
    Protected Sub Click_Confirm(ByVal sender As System.Object, ByVal e As System.EventArgs)

        SuccessText = "&nbsp;"
        ErrorText = String.Empty
        Try
            RaiseEvent Save()

            Me.ConfirmPopUp.Dispose()
        Catch
            Throw
        End Try
    End Sub

